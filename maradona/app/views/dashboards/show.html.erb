<h1> HIHI =>>> <%= previous_url%></h1>

<%= render 'dashboard' %>

<%= params %>
<%# Creates submit peer request button%>
<% if @peer_request %>
	<%= form_for([@peer_request]) do |f| %>  
		<div class="actions">
		  <%= f.submit 'Submit Peer Request' %>
		  
		</div>
	<% end %>
<% elsif @peership %>
	<p> You are peers.</p>
<% else %>
	<p> Peer request has been sent; pending reply </p>
<% end %>	

<h1> wall posts</h1>
<%= render 'shared/post_form' %>
<% if @user.posts.any? %>
      <h3>Wall Posts Created(<%= @posts.count %>)</h3>
      <ol>
        <% for el in @posts %>
	<%if el.other_users == "" or el.other_users == nil%>
		<%= @user.profile.name %> said:
	<%else%>
		<b> <%= User.find(el.user_id).profile.name%> </b> said:
	<%end%>

		<%=el.content %>
		Posted <%= time_ago_in_words(el.created_at) %> ago.
	<br />
	<br />
	<%end%>
      </ol>
      <%= will_paginate @posts %>
<% end %>
